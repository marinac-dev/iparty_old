    <style>
      .glitch {
        color: white;
        position: relative;
        margin: 0 auto;
        font-family: "Varela", sans-serif;
      }

      @keyframes noise-anim {
        0% {
          clip-path: inset(39% 0 18% 0);
        }
        5% {
          clip-path: inset(35% 0 46% 0);
        }
        10% {
          clip-path: inset(5% 0 80% 0);
        }
        15% {
          clip-path: inset(84% 0 10% 0);
        }
        20% {
          clip-path: inset(35% 0 7% 0);
        }
        25% {
          clip-path: inset(3% 0 67% 0);
        }
        30% {
          clip-path: inset(89% 0 3% 0);
        }
        35% {
          clip-path: inset(50% 0 42% 0);
        }
        40% {
          clip-path: inset(79% 0 19% 0);
        }
        45% {
          clip-path: inset(82% 0 13% 0);
        }
        50% {
          clip-path: inset(76% 0 15% 0);
        }
        55% {
          clip-path: inset(77% 0 24% 0);
        }
        60% {
          clip-path: inset(64% 0 8% 0);
        }
        65% {
          clip-path: inset(25% 0 29% 0);
        }
        70% {
          clip-path: inset(7% 0 67% 0);
        }
        75% {
          clip-path: inset(69% 0 11% 0);
        }
        80% {
          clip-path: inset(94% 0 4% 0);
        }
        85% {
          clip-path: inset(87% 0 10% 0);
        }
        90% {
          clip-path: inset(16% 0 49% 0);
        }
        95% {
          clip-path: inset(17% 0 68% 0);
        }
        100% {
          clip-path: inset(59% 0 14% 0);
        }
      }
      .glitch::after {
        content: attr(data-text);
        position: absolute;
        left: 2px;
        text-shadow: -1px 0 red;
        top: 0;
        color: white;
        background: black;
        overflow: hidden;
        animation: noise-anim 2s infinite linear alternate-reverse;
      }

      @keyframes noise-anim-2 {
        0% {
          clip-path: inset(2% 0 90% 0);
        }
        5% {
          clip-path: inset(43% 0 57% 0);
        }
        10% {
          clip-path: inset(74% 0 25% 0);
        }
        15% {
          clip-path: inset(98% 0 2% 0);
        }
        20% {
          clip-path: inset(91% 0 10% 0);
        }
        25% {
          clip-path: inset(26% 0 27% 0);
        }
        30% {
          clip-path: inset(43% 0 38% 0);
        }
        35% {
          clip-path: inset(98% 0 2% 0);
        }
        40% {
          clip-path: inset(84% 0 5% 0);
        }
        45% {
          clip-path: inset(11% 0 60% 0);
        }
        50% {
          clip-path: inset(41% 0 13% 0);
        }
        55% {
          clip-path: inset(38% 0 12% 0);
        }
        60% {
          clip-path: inset(24% 0 15% 0);
        }
        65% {
          clip-path: inset(22% 0 57% 0);
        }
        70% {
          clip-path: inset(50% 0 13% 0);
        }
        75% {
          clip-path: inset(29% 0 19% 0);
        }
        80% {
          clip-path: inset(79% 0 7% 0);
        }
        85% {
          clip-path: inset(77% 0 19% 0);
        }
        90% {
          clip-path: inset(75% 0 3% 0);
        }
        95% {
          clip-path: inset(96% 0 5% 0);
        }
        100% {
          clip-path: inset(85% 0 2% 0);
        }
      }
      .glitch::before {
        content: attr(data-text);
        position: absolute;
        left: -2px;
        text-shadow: 1px 0 blue;
        top: 0;
        color: white;
        background: black;
        overflow: hidden;
        animation: noise-anim-2 15s infinite linear alternate-reverse;
      }
      /* Button */
      .rainbow-button {
        width: calc(20vw + 6px);
        height: calc(8vw + 6px);
        background-image: linear-gradient(90deg, #00c0ff 0%, #ffcf00 49%, #fc4f4f 80%, #00c0ff 100%);
        border-radius: 5px;
        animation: slidebg 900ms linear infinite;
      }
      .rainbow-button:after {
        content: attr(alt);
        background-color: black;
      }

      @keyframes slidebg {
        to {
          background-position: 12rem;
        }
      }
    </style>
  </head>
  <body>
    <header class="flex items-center justify-center mx-4 my-2 rounded py-2 px-3 shadow font-sans tracking-wide relative bg-black">
      <div class="flex flex-row w-full lg:w-auto sm:w-full justify-between items-center">
        <!-- Logo -->
        <div>
          <a href="/">
            <img class="h-12" src="/images/logos/logo-white.png" alt="iParty logo" />
          </a>
        </div>
      </div>
    </header>

    <!-- Content -->
    <div class="flex justify-center items-center rounded shadow h-screen bg-black text-gray-900 dark:text-sage-500 mx-4 my-2 py-2 px-3">
      <div class="flex flex-col justify-center items-center">
        <p class="uppercase text-white text-2xl">
          Error
        </p>
        <p class="glitch text-6xl" data-text="404">404</p>
        <div class="flex flex-row justify-center items-center lowercase">
          <p class="glitch text-3xl pr-8" data-text="page">page</p>
          <p class="glitch text-3xl pr-8" data-text="not">not</p>
          <p class="glitch text-3xl" data-text="found">found</p>
        </div>
        <p class="uppercase text-lg text-white">
          Go
          <a href="/" class="text-rich-black mx-10 text-3xl tracking-widest hover:text-pink-500 rainbow-button">HOME</a>
        </p>
      </div>
    </div>
  </body>
</html>
